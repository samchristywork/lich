(func! print-matrix! (m)
  (if (= (length m) 0)
      true
      (&&
        (write! (head m))
        (print-matrix! (tail m)))))

(set matrix ('
  (1 2 3 4 5 6)
  (2 2 3 4 5 6)
  (3 2 3 4 5 6))
(

(write! "This is the matrix:")
(print-matrix! matrix)

(write!)
(write! "This is the filtered matrix:")
(func! f (r)
  (odd? (head r)))
(print-matrix! (filter f matrix))

(write!)
(write! "This is the lambda-filtered matrix:")
(write! "foo" (head (head (head (' matrix)))))
(write! "---")
(! ((lambda (r) (odd? (head r))) (' 1 2 3)))
))
